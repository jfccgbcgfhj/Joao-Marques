algoritmo "COversao_de_Cº_para_Fº"
// Autor : João Marques
// Data : 18/09/2017
// Cálculo da formula de conversao de Cº para Fº
var
    C,F : real
inicio
      escreva("Qual a temperatuda de C? ")
      leia(C)
      F <- C * 3.875
      escreva("A temperatura convertida é",F)
fimalgoritmo
